var HomeComponent = /** @class */ (function () {
    function HomeComponent(http, storage, activatedRoute) {
        this.http = http;
        this.storage = storage;
        this.activatedRoute = activatedRoute;
        this.userData = [];
        this.editCache = {};
        this.allChecked = false;
        this.indeterminate = false;
        this.isSelected = 'home';
        this.tags = [];
        this.tagChecked = '用户列表';
    }
    HomeComponent.prototype.startEdit = function (key) {
        this.editCache[key].edit = true;
    };
    HomeComponent.prototype.refreshStatus = function () {
        var allChecked = this.userData.every(function (value) { return value.checked === true; });
        var allUnChecked = this.userData.every(function (value) { return !value.checked; });
        this.allChecked = allChecked;
        this.indeterminate = (!allChecked) && (!allUnChecked);
    };
    HomeComponent.prototype.checkAll = function (value) {
        this.userData.forEach(function (data) { return data.checked = value; });
        this.refreshStatus();
    };
    HomeComponent.prototype.cancelEdit = function (key) {
        this.editCache[key].edit = false;
    };
    HomeComponent.prototype.saveEdit = function (key) {
        this.editCache[key].edit = false;
    };
    HomeComponent.prototype.ngOnInit = function () {
        var _this = this;
        this.http.post('/api/selectAllUser', {}).subscribe(function (res) {
            console.log(res);
            if (res.code == 0) {
                // let len=res.data.length;
                // for(let i=0;i<len;i++){
                //   this.userData.push(res.data[i]);
                // }
                _this.userData = res.data;
                res.data.forEach(function (item) {
                    if (!_this.editCache[(item.userId)]) {
                        _this.editCache[(item.userId)] = {
                            edit: false,
                            data: tslib__WEBPACK_IMPORTED_MODULE_0__["__assign"]({}, item)
                        };
                    }
                });
                console.log(_this.editCache);
            }
        }, function (err) { console.log(err); });
    };
    HomeComponent = tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"]([
        Object(_angular_core__WEBPACK_IMPORTED_MODULE_1__["Component"])({
            selector: 'app-home',
            template: __webpack_require__(/*! ./home.component.html */ "./src/app/home/home/home.component.html"),
            styles: [__webpack_require__(/*! ./home.component.scss */ "./src/app/home/home/home.component.scss")]
        }),
        tslib__WEBPACK_IMPORTED_MODULE_0__["__metadata"]("design:paramtypes", [_angular_common_http__WEBPACK_IMPORTED_MODULE_2__["HttpClient"], _service_storage_service__WEBPACK_IMPORTED_MODULE_3__["StorageService"], _angular_router__WEBPACK_IMPORTED_MODULE_4__["ActivatedRoute"]])
    ], HomeComponent);
    return HomeComponent;
}());



