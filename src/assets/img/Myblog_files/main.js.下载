var ArticleCategoryComponent = /** @class */ (function () {
    function ArticleCategoryComponent(activatedRoute, http, message) {
        this.activatedRoute = activatedRoute;
        this.http = http;
        this.message = message;
        this.isVisible = false;
    }
    ArticleCategoryComponent.prototype.getCategoryList = function () {
        var _this = this;
        this.http.post("/api/selectAllCategory", '').subscribe(function (res) {
            console.log(res);
            if (res.code == 0) {
                _this.categoryList = res.data;
            }
        }, function (err) { });
    };
    ArticleCategoryComponent.prototype.append = function (id) {
        this.isVisible = true;
        this.categoryPid = id;
    };
    ArticleCategoryComponent.prototype.handleOk = function () {
        var _this = this;
        var data = {
            categoryPid: this.categoryPid,
            categoryName: this.categoryName,
            categoryDescription: this.categoryDescription,
            categoryIcon: this.categoryIcon
        };
        this.http.post('/api/insertCategory', data).subscribe(function (res) {
            console.log(res);
            if (res.code == 0) {
                _this.message.create('success', '新增类别成功');
                _this.getCategoryList();
                _this.isVisible = false;
            }
        }, function (err) { });
    };
    ArticleCategoryComponent.prototype.handleCancel = function () {
        this.isVisible = false;
    };
    /**
     * 单条删除
     * @param id
     */
    ArticleCategoryComponent.prototype.delete = function (id) {
        var _this = this;
        var data = {
            categoryId: id
        };
        this.http.post("/api/deleteCategory", data).subscribe(function (res) {
            if (res.code == 0) {
                _this.message.create('success', '删除成功');
                _this.getCategoryList();
            }
        }, function (err) { });
    };
    ArticleCategoryComponent.prototype.deleteAll = function (id) {
        console.log(id);
        var data = {
            categoryId: id
        };
        this.http.post("/api/selectChildren", data).subscribe(function (res) {
            console.log(res);
        }, function (err) { });
    };
    ArticleCategoryComponent.prototype.ngOnInit = function () {
        var _this = this;
        this.getCategoryList();
        this.activatedRoute.queryParams.subscribe(function (queryParams) {
            _this.content = queryParams.content;
        });
    };
    ArticleCategoryComponent = tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"]([
        Object(_angular_core__WEBPACK_IMPORTED_MODULE_1__["Component"])({
            selector: 'app-article-category',
            template: __webpack_require__(/*! ./article-category.component.html */ "./src/app/article/article-category/article-category.component.html"),
            styles: [__webpack_require__(/*! ./article-category.component.scss */ "./src/app/article/article-category/article-category.component.scss")]
