/***/ "./src/app/service/storage.service.ts":
/*!********************************************!*\
  !*** ./src/app/service/storage.service.ts ***!
  \********************************************/
/*! exports provided: StorageService */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "StorageService", function() { return StorageService; });
/* harmony import */ var tslib__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! tslib */ "./node_modules/tslib/tslib.es6.js");
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! @angular/core */ "./node_modules/@angular/core/fesm5/core.js");


var StorageService = /** @class */ (function () {
    function StorageService() {
    }
    StorageService.prototype.setUserId = function (userId) {
        localStorage.setItem("userId", userId.toString());
    };
    StorageService.prototype.getUserId = function () {
        return localStorage.getItem("userId");
    };
    StorageService.prototype.setPaths = function (data) {
        this.tagChecked = data;
        console.log(data);
        if (localStorage.getItem("paths") == null) {
            var arr = new Array();
            localStorage.setItem("paths", JSON.stringify(arr));
            var paths = localStorage.getItem("paths");
            if (arr.indexOf(data) == -1) {
                // 当前标签不存在paths中时
                arr.push(data);
                console.log(arr);
                localStorage.setItem('paths', JSON.stringify(arr));
            }
            else {
                // 当前存在，置顶
                this.swapArray(JSON.parse(paths), JSON.parse(paths).indexOf(data));
            }
        }
        else {
            var paths = JSON.parse(localStorage.getItem("paths"));
            console.log('运行到else');
            if (paths.indexOf(data) == -1) {
                // 当前标签不存在paths中时
                paths.push(data);
                localStorage.setItem('paths', JSON.stringify(paths));
            }
            else {
                // 当前存在，置顶
                console.log('运行到置顶');
                this.swapArray(paths, paths.indexOf(data));
            }
        }
    };
    StorageService.prototype.getTagChecked = function () {
        console.log(this.tagChecked);
        return this.tagChecked;
    };
    StorageService.prototype.getPaths = function () {
        if (localStorage.getItem("paths") != null && localStorage.getItem("paths") != '[]') {
            return JSON.parse(localStorage.getItem("paths"));
        }
    };
    StorageService.prototype.removePath = function (tag) {
        if (localStorage.getItem("paths") != null && localStorage.getItem("paths") != '[]') {
            var paths = JSON.parse(localStorage.getItem("paths"));
            paths.splice(paths.indexOf(tag), 1);
            localStorage.setItem("paths", JSON.stringify(paths));
        }
    };
    // 置顶
    StorageService.prototype.swapArray = function (arr, index2) {
        arr[arr.length - 1] = arr.splice(index2, 1)[0];
        localStorage.setItem("paths", JSON.stringify(arr));
    };
    StorageService = tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"]([
        Object(_angular_core__WEBPACK_IMPORTED_MODULE_1__["Injectable"])({
            providedIn: 'root'
        }),
        tslib__WEBPACK_IMPORTED_MODULE_0__["__metadata"]("design:paramtypes", [])
    ], StorageService);
    return StorageService;
}());



