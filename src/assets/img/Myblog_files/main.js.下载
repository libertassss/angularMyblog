var ArticleListComponent = /** @class */ (function () {
    function ArticleListComponent(http, router) {
        this.http = http;
        this.router = router;
        this.articleData = [];
        this.editCache = {};
        this.allChecked = false;
        this.indeterminate = false;
    }
    ArticleListComponent.prototype.refreshStatus = function () {
        var allChecked = this.articleData.every(function (value) { return value.checked === true; });
        var allUnChecked = this.articleData.every(function (value) { return !value.checked; });
        this.allChecked = allChecked;
        this.indeterminate = (!allChecked) && (!allUnChecked);
    };
    ArticleListComponent.prototype.checkAll = function (value) {
        this.articleData.forEach(function (data) { return data.checked = value; });
        this.refreshStatus();
    };
    ArticleListComponent.prototype.startEdit = function (key) {
        this.editCache[key].edit = true;
    };
    ArticleListComponent.prototype.cancelEdit = function (key) {
        this.editCache[key].edit = false;
    };
    ArticleListComponent.prototype.saveEdit = function (key) {
        this.editCache[key].edit = false;
    };
    ArticleListComponent.prototype.detaile = function (content) {
        this.router.navigate(['/parents/articleCategory'], {
            queryParams: {
                content: content
            }
        });
    };
    ArticleListComponent.prototype.more = function (item) {
    };
    ArticleListComponent.prototype.edit = function (item) {
    };
    ArticleListComponent.prototype.delete = function (id) {
        var data = [];
        data.push(id);
        var datas = {
            item: data
        };
        this.http.post("/api/deleteArticleMall", datas).subscribe(function (res) {
            console.log(res);
        }, function (err) { });
    };
    ArticleListComponent.prototype.editArticle = function () {
        console.log(1);
        this.router.navigateByUrl('/parents/newArticle');
    };
    ArticleListComponent.prototype.ngOnInit = function () {
        var _this = this;
