        this.tags = null;
        this.tinyMceSettings = {};
    }
    NewArticleComponent.prototype.handleEvent = function (e) {
        console.log(this.articleContent);
        console.log(e.event.target.body.innerText);
        this.articleContentMd = e.event.target.body.innerText;
    };
    NewArticleComponent.prototype.onChanges = function (values) {
    };
    NewArticleComponent.prototype.save = function () {
        var len = this.categoryIds.length;
        var data = {
            articleUserId: this.storage.getUserId(),
            articleTitle: this.articleTitle,
            articleContent: this.articleContent,
            articleContentMd: this.articleContentMd,
            articleParentCategoryId: this.categoryIds[0],
            articleChildCategoryId: this.categoryIds[len - 1],
            articleTagIds: this.selectTagIds.join(",")
        };
        this.http.post('/api/insertSelectev', data).subscribe(function (res) {
            console.log(res);
        }, function (err) { });
    };
    // 级联选择器
    NewArticleComponent.prototype.fun = function (e) {
        console.log(e);
        if (e.option.children.length <= 0) {
            e.option.isLeaf = true;
        }
        else {
        }
    };
    NewArticleComponent.prototype.getCategory = function () {
        var _this = this;
        this.http.post('/api/selectAllCategory', '').subscribe(function (res) {
            if (res.code == 0) {
                _this.categoryList = res.data;
                _this.categoryIds = [_this.categoryList[0].id];
                console.log(res);
                console.log(_this.categoryList);
            }
        }, function (err) { });
    };
    NewArticleComponent.prototype.getTags = function () {
        var _this = this;
        this.http.post('/api/selectAllTags', '').subscribe(function (res) {
            if (res.code == 0) {
                console.log(res);
                _this.tags = res.data;
            }
        }, function (err) {
        });
    };
    // 图片上传
    NewArticleComponent.prototype.uploadImag = function (data, success, failure) {
        this.http.post('/api/uploadImg', data).subscribe(function (res) {
            if (res.code == 0) {
                success("/api" + res.url);
            }
            else {
                failure(res.url);
            }
        }, function (err) { });
    };
