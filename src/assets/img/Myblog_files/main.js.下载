/***/ "./src/app/navigation/blog-nav/blog-nav.component.ts":
/*!***********************************************************!*\
  !*** ./src/app/navigation/blog-nav/blog-nav.component.ts ***!
  \***********************************************************/
/*! exports provided: BlogNavComponent */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "BlogNavComponent", function() { return BlogNavComponent; });
/* harmony import */ var tslib__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! tslib */ "./node_modules/tslib/tslib.es6.js");
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! @angular/core */ "./node_modules/@angular/core/fesm5/core.js");
/* harmony import */ var _angular_common_http__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! @angular/common/http */ "./node_modules/@angular/common/fesm5/http.js");



var BlogNavComponent = /** @class */ (function () {
    function BlogNavComponent(http) {
        this.http = http;
        this.isVisible = false;
        this.editCache = {};
        this.allChecked = false;
        this.indeterminate = false;
    }
    BlogNavComponent.prototype.handleCancel = function () {
        this.isVisible = false;
    };
    BlogNavComponent.prototype.handleOk = function () {
        var data = {
            menuName: this.menuName,
            menuLever: this.menuLever,
            menuIcon: this.menuIcon
        };
        this.http.post("/api/insertMenuItem", data).subscribe(function (res) {
            console.log(res);
        }, function (err) { });
    };
    BlogNavComponent.prototype.getMenuList = function () {
        var _this = this;
        this.http.post("/api/selecteMenu", '').subscribe(function (res) {
            console.log(res);
            if (res.code == 0) {
                _this.menuList = res.data;
                res.data.forEach(function (item) {
                    if (!_this.editCache[(item.menuId)]) {
                        _this.editCache[(item.menuId)] = {
                            edit: false,
                            data: tslib__WEBPACK_IMPORTED_MODULE_0__["__assign"]({}, item)
                        };
                    }
                });
            }
        }, function (err) { });
    };
    BlogNavComponent.prototype.refreshStatus = function () {
        var allChecked = this.menuList.every(function (value) { return value.checked === true; });
        var allUnChecked = this.menuList.every(function (value) { return !value.checked; });
        this.allChecked = allChecked;
        this.indeterminate = (!allChecked) && (!allUnChecked);
    };
    BlogNavComponent.prototype.checkAll = function (value) {
        this.menuList.forEach(function (data) { return data.checked = value; });
        this.refreshStatus();
    };
    BlogNavComponent.prototype.startEdit = function (key) {
        this.editCache[key].edit = true;
    };
    BlogNavComponent.prototype.cancelEdit = function (key) {
        this.editCache[key].edit = false;
    };
    BlogNavComponent.prototype.saveEdit = function (key) {
        this.editCache[key].edit = false;
    };
    BlogNavComponent.prototype.ngOnInit = function () {
        this.getMenuList();
    };
    BlogNavComponent = tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"]([
        Object(_angular_core__WEBPACK_IMPORTED_MODULE_1__["Component"])({
            selector: 'app-blog-nav',
            template: __webpack_require__(/*! ./blog-nav.component.html */ "./src/app/navigation/blog-nav/blog-nav.component.html"),
